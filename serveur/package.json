{
  "name": "tunisia-construction-finder",
  "version": "1.0.0",
  "description": "MVP pour trouver des entreprises de construction et fournisseurs tunisiens",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "init-env": "node scripts/init-env.js",
    "check-deps": "node scripts/check-deps.js",
    "check-env": "node check-env.js",
    "reset-db": "node scripts/reset-db.js",
    "migrate-to-mongo": "ts-node --transpile-only scripts/migrate-to-mongo.ts",
    "clean-duplicates": "ts-node --transpile-only scripts/clean-duplicates.ts",
    "create-user": "ts-node --transpile-only scripts/create-user.ts"
  },
  "keywords": [
    "tunisia",
    "construction",
    "scraper",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "antd": "^5.28.0",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^9.2.2",
    "cors": "^2.8.5",
    "diacritics": "^1.3.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "he": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "natural": "^6.10.4",
    "node-fetch": "^2.7.0",
    "p-limit": "^3.1.0",
    "p-retry": "^4.6.2",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "react-router-dom": "^7.9.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/diacritics": "^1.3.3",
    "@types/express": "^4.17.21",
    "@types/he": "^1.2.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/natural": "^5.1.5",
    "@types/node": "^20.10.6",
    "@types/node-fetch": "^2.6.9",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
